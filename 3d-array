#include <stdio.h>

int main() {
    int x = 2, y = 2, z = 2;
    int array[2][2][2] = {
        {{10, 20}, {30, 40}},
        {{50, 60}, {70, 80}}
    };

    printf("Elements of the 3D array:\n");
    for (int i = 0; i < x; i++) {
        for (int j = 0; j < y; j++) {
            for (int k = 0; k < z; k++) {
                printf("array[%d][%d][%d] = %d\n", i, j, k, array[i][j][k]);
            }
        }
    }

    return 0;
}
